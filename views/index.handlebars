<div class="col-md-4 col-md-offset-2">



{{#each burgers}}
<p>{{burger_name}}</p>
{{/each}}
<ul>
        <h2>Menu</h2>

        {{#each burgers}} 

        {{#unless complete}} 
        {{> burgers/burger-block complete=true}} 
        {{/unless}} 
        
        {{/each}}
    </ul>

</div>


<div class="col-md-4 col-md-offset-1">
    <ul>
        <h2>Eaten</h2>

        {{#each burgers}} {{#if complete}} {{> burgers/burger-block complete=false}} {{/if}} {{/each}}
    </ul>
</div>